{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@p/*": ["src/assets/*"],
      "@scripts/*": ["src/scripts/*"],
      "@fav/*": ["public/assets/favicons/*"],
      "@svgs/*": ["src/assets/svgs/*"],
      "@data/*": ["src/data/*"],
      "@static/*": ["public/assets/*"],
      "@style/*": ["src/style/*"],
      "@content/*": ["src/content/*"]
    }
  }
}
