---
import { Image } from 'astro:assets'

export interface Props {
  p: any
  alt: string
  quality: string | 'low' //low, mid, high, max
  cssClass: string
}

const { p, alt, quality, cssClass } = Astro.props
---

<Image
  class={cssClass}
  src={p}
  width={1920}
  alt={alt}
  quality={quality}
  format="avif"
/>
